drop database if exists sampledb;
create database sampledb;
use sampledb;

use itemdb;

create table sample_table(
No int, name varchar(50), suu int(50));


create table item_table(
No int, name varchar(50), check int(50));


insert into sample_table values(1,"商品1", 21);
insert into sample_table values(2,"商品2", 39);
insert into sample_table values(3,"商品3", 44);
insert into sample_table values(4,"商品4", 44);


insert into item_table values(1,"shouhin1",21);