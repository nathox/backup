<script language=javascript>
function obakasannemou(){
objChk = frmMove.chkMove //チェックボックスオブジェクトを参照
str = ""; //チェック内容を代入する変数
for(i = 0 ; i < objChk.length;i++){
if(objChk[i].checked) str += objChk[i].value
}
switch(str){
case "1":
location.href="a.html";
break;
case "2":
location.href="b.html";
break;

case "3":
location.href="a.html";
break;

case "4":
location.href="c.html";
break;

case "5":
location.href="c.html";
break;

case "125":
location.href="c.html";
break;
}
}
</script>
